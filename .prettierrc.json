{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "printWidth": 100,
  "endOfLine": "lf",
  "bracketSameLine": true,
  "importOrder": [
    "^server-only$",
    "^node:",
    "^(react|react-dom)(/|$)",
    "<THIRD_PARTY_MODULES>",
    "^~/",
    "^./",
    "\\.css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
