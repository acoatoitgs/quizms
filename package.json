{
  "name": "@olinfo/quizms",
  "version": "1.0.7",
  "type": "module",
  "author": "Alessandro Bortolin <bortolin.alessandro@outlook.it>",
  "license": "MIT",
  "scripts": {
    "prebuild": "eslint . && prettier --check --cache --log-level=warn src/",
    "build": "NODE_ENV=production node scripts/build.js build",
    "watch": "node scripts/build.js watch",
    "lint": "eslint --fix . && tsc --noEmit && prettier --write --cache --log-level=warn src/",
    "fmt": "prettier --write --cache src/",
    "prepack": "yarn build",
    "release": "npm publish --access public"
  },
  "bin": {
    "quizms": "bin/bin.js"
  },
  "exports": {
    "./admin": "./dist/admin/index.js",
    "./student": "./dist/student/index.js",
    "./teacher": "./dist/teacher/index.js",
    "./firebase": "./dist/firebase/index.js",
    "./css": "./dist/index.css",
    "./jsx-runtime": "./dist/jsx-runtime/index.js"
  },
  "typesVersions": {
    "*": {
      "admin": [
        "./dist/admin/index.d.ts"
      ],
      "student": [
        "./dist/student/index.d.ts"
      ],
      "teacher": [
        "./dist/teacher/index.d.ts"
      ],
      "firebase": [
        "./dist/firebase/index.d.ts"
      ]
    }
  },
  "files": [
    "bin",
    "dist"
  ],
