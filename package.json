{
  "name": "quizms",
  "version": "1.0.0-pre-alpha.1",
  "type": "module",
  "private": true,
  "author": "Alessandro Bortolin <bortolin.alessandro@outlook.it>",
  "license": "MIT",
  "scripts": {
    "build": "node esbuild.js build",
    "watch": "node esbuild.js watch",
    "lint": "eslint --fix . ; tsc --noEmit",
    "fmt": "prettier --write src/"
  },
  "main": "dist/index.js",
  "bin": "bin/bin.js",
  "exports": {
    ".": "./dist/index.js",
    "./css": "./dist/index.css",
    "./jsx-runtime": "./dist/jsx-runtime.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "dependencies": {
    "@blockly/disable-top-blocks": "^0.4",
    "@mdx-js/mdx": "^2.3",
    "@mdx-js/rollup": "^2.3",
    "@swc/plugin-transform-imports": "^1.5",
    "@vitejs/plugin-react-swc": "^3.3",
    "acorn": "^8.10",
    "blockly": "^10.0",
    "classnames": "^2.3",
    "commander": "^11.0",
    "dayjs": "^1.11",
    "estree-toolkit": "^1.6",
    "estree-util-to-js": "^2.0",
    "fast-glob": "^3.3.1",
    "hast-util-to-text": "^3.0",
    "js-interpreter": "^4.0",
    "lodash": "^4.17",
    "lowlight": "^3.0",
    "mdast-util-mdxjs-esm": "=1.3.1",
    "mini-svg-data-uri": "^1.4",
    "playwright": "^1.36",
    "prop-types": "^15.8",
    "pure-rand": "^6.0",
    "react-blockly": "^8.0",
    "react-feather": "^2.0",
    "react-katex": "^3.0",
    "remark-frontmatter": "^4.0",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1",
    "remark-mdx-frontmatter": "^2.1",
    "remark-mdx-math-enhanced": "=0.0.1-beta.3",
    "remark-smartypants": "^2.0",
    "sharp": "^0.32",
    "svgo": "^3.0",
    "swr": "^2.2",
    "tempy": "^3.1",
    "unified": "^10.1",
    "unist-util-visit": "^5.0",
    "vite": "^4.4",
    "vm-browserify": "^1.1",
    "zod": "^3.21"
  },
  "peerDependencies": {
    "@mdx-js/react": "^2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5",
    "@trivago/prettier-plugin-sort-imports": "^4.1",
    "@types/lodash": "^4.14",
    "@types/node": "^20",
    "@types/react": "^18.0",
    "@types/react-katex": "^3.0",
    "@typescript-eslint/eslint-plugin": "^6.2",
    "@typescript-eslint/parser": "^6.2",
    "daisyui": "^3.2",
    "esbuild": "^0.18",
    "eslint": "^8.34",
    "eslint-plugin-react": "^7.32",
    "eslint-plugin-react-hooks": "^4.6",
    "highlight.js": "^11.8",
    "less": "^4.1",
    "postcss": "^8.4",
    "prettier": "^3.0",
    "prettier-plugin-tailwindcss": "^0.4",
    "tailwindcss": "^3.2",
    "typescript": "^5.1"
  },
  "browserslist": [
    "defaults, >1% in IT, last 3 versions, not dead",
    "maintained node versions"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "bracketSameLine": true,
    "importOrder": [
      "^server-only$",
      "^node:",
      "^(react|react-dom)(/|$)",
      "<THIRD_PARTY_MODULES>",
      "^(@|~)/",
      "^..?/",
      "\\.css$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "ignorePatterns": [
      "*.js",
      "dist/"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "linebreak-style": "off",
      "quotes": "off",
      "semi": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  }
}
