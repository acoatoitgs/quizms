{
  "name": "quizms",
  "version": "1.0.0-pre-alpha.1",
  "private": true,
  "author": "Alessandro Bortolin <bortolin.alessandro@outlook.it>",
  "license": "MIT",
  "scripts": {
    "build": "parcel build",
    "watch": "parcel build --no-optimize && parcel watch -p 1235",
    "lint": "eslint --fix . ; tsc --noEmit",
    "fmt": "prettier --write src/"
  },
  "asy-loader": "dist/asy-loader/index.js",
  "cli": "dist/cli.mjs",
  "main": "dist/index.mjs",
  "mdx": "dist/mdx/index.mjs",
  "targets": {
    "asy-loader": {
      "source": "src/asy-loader/index.ts",
      "outputFormat": "commonjs",
      "context": "node",
      "isLibrary": true,
      "optimize": true
    },
    "cli": {
      "source": "src/cli/index.ts",
      "context": "node",
      "outputFormat": "esmodule",
      "isLibrary": true,
      "optimize": true
    },
    "css": {
      "source": "src/css/index.css",
      "isLibrary": true,
      "optimize": true
    },
    "main": {
      "source": "src/index.ts",
      "context": "browser",
      "outputFormat": "esmodule",
      "isLibrary": true,
      "optimize": true
    },
    "mdx": {
      "source": "src/mdx/index.ts",
      "context": "node",
      "outputFormat": "esmodule",
      "isLibrary": true,
      "optimize": true
    }
  },
  "browserslist": [
    "defaults, >0.1% in IT, last 3 versions, not dead",
    "maintained node versions"
  ],
  "files": [
    "bin",
    "dist"
  ],
  "bin": "bin/bin.js",
  "exports": {
    ".": "./dist/index.mjs",
    "./asy-loader": "./dist/asy-loader/index.js",
    "./css": "./dist/css/index.css",
    "./mdx": "./dist/mdx/index.mjs"
  },
  "dependencies": {
    "@headlessui/react": "^1.7",
    "@heroicons/react": "^2.0",
    "@mdx-js/mdx": "^2.3",
    "@mdx-js/react": "^2.3",
    "@swc/core": "^1.3",
    "@swc/plugin-transform-imports": "^1.5",
    "@swc/wasm": "^1.3",
    "classnames": "^2.3",
    "colors": "^1.4",
    "dayjs": "^1.11",
    "estree-util-visit": "^1.2",
    "lodash": "^4.17.21",
    "mdast-util-mdxjs-esm": "^1.3",
    "mini-svg-data-uri": "^1.4",
    "prop-types": "^15.8",
    "react-katex": "^3.0",
    "rehype-raw": "^6.1",
    "remark-frontmatter": "^4.0",
    "remark-math": "^5.1",
    "remark-mdx-frontmatter": "^2.1",
    "remark-mdx-images": "^2.0",
    "remark-mdx-math-enhanced": "=0.0.1-beta.3",
    "remark-smartypants": "^2.0",
    "swc-loader": "^0.2",
    "terser-webpack-plugin": "^5.3",
    "unified": "^10.1",
    "unist-util-visit": "^4.1",
    "webpack": "^5.76"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5",
    "@tailwindcss/typography": "^0.5",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/lodash": "^4.14.191",
    "@types/mdast": "^3.0",
    "@types/node": "^18.14",
    "@types/react": "^18.0",
    "@types/react-katex": "^3.0",
    "@typescript-eslint/eslint-plugin": "^5.53",
    "@typescript-eslint/parser": "^5.53",
    "eslint": "^8.34",
    "eslint-plugin-react": "^7.32",
    "eslint-plugin-react-hooks": "^4.6",
    "parcel": "=2.7.0",
    "postcss": "^8.4",
    "prettier": "^2.8",
    "prettier-plugin-tailwindcss": "^0.2",
    "react": "^18.2",
    "react-dom": "^18.2",
    "tailwindcss": "^3.2",
    "typescript": "^4.9"
  }
}
